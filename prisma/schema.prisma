// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  discord_id String @id
  discord_name String
  coins Int @default(100)
}

model account {
  discord_id String @id
  discord_name String
  coins Int @default(0)
  history String @default("[]")
}
